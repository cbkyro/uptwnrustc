{"components/faceted-search/facets/hierarchy":"<div class=\"facet-filter-wrapper {{#unless start_collapsed}}is-open{{/unless}}\" data-facet-filter-wrapper>\n  <ul\n    id=\"faceted-search-nav-list-{{#replace ' ' facet}}{{else}}{{facet}}{{/replace}}\"\n    class=\"facet-list-items {{#if selected}}is-open{{/if}} {{#unless start_collapsed}}is-open{{/unless}}\"\n    data-count=\"{{items_count}}\" data-facet=\"{{facet}}\">\n    {{#each items}}\n      <li class=\"facet-item\" data-facet-item>\n        <a href=\"{{url}}\" rel=\"nofollow\" data-faceted-search-facet>\n          {{title}}\n          {{#if ../show_product_counts}}<span data-facet-item-count>({{count}})</span>{{/if}}\n        </a>\n      </li>\n    {{/each}}\n  </ul>\n\n  {{#if show_more toggle}}\n    <a href=\"#faceted-search-nav-list-{{ facet }}\" role=\"button\"\n      class=\"facet-toggle-more\" href=\"#\" rel=\"nofollow\"\n      data-show-more-facets>\n      {{lang 'faceted_search.show_more'}}\n      <svg class=\"icon icon-arrow-down\"><use xlink:href=\"#icon-arrow-down\" /></svg>\n    </a>\n  {{/if}}\n</div>\n"}